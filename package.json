{
  "name": "schmupinator",
  "private": true,
  "type": "module",
  "version": "0.7.0",
  "scripts": {
    "dev": "vite --port 3000 --open",
    "build": "vite build",
    "build:release": "vite build --mode release --base=/schmupinator/ --outDir dist/schmupinator",
    "preview": "vite preview --port 3000 --open",
    "preview:release": "vite preview --port 3000 --open schmupinator/",
    "e2e:vite-node": "vite-node ./src/main.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.12.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "globals": "^15.11.0",
    "terser": "^5.34.1",
    "type-fest": "^4.13.0",
    "typescript": "5.2.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.2.14",
    "vite-node": "^2.1.2",
    "vitest": "^2.1.2"
  },
  "optionalDependencies:comments": "needed only for one reason. lock file created and committed in windows makes pipelines explode in linux",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  }
}